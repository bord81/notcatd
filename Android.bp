rust_binary {
    name: "notcatd",

    srcs: ["src/main.rs"],
    crate_name: "notcatd",
    edition: "2021",
    rustlibs: [
        "libnix",
        "librustutils",
        "libtokio",
        "libthiserror",
    ],
    shared_libs: [
       "liblog",
    ],
    system_ext_specific: true,
    init_rc: ["notcatd.rc"],
}


